{
  "name": "@bytekit/totp",
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "git@github.com:neoscrib/bytekit-totp.git"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "index.d.ts",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "otp",
    "totp",
    "hotp",
    "authenticator",
    "google authenticator",
    "2fa"
  ],
  "scripts": {
    "test": "vitest run --coverage",
    "test:browser": "vitest run -c vitest.browser.config.ts",
    "test:browser:watch": "vitest -c vitest.browser.config.ts",
    "test:browser:ui": "vitest --ui --browser -c vitest.browser.config.ts",
    "test:all": "yarn test && yarn test:browser",
    "build": "vite build",
    "lint": "eslint ."
  },
  "author": "Tanner Jepsen <neoscrib@gmail.com>",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@stryker-mutator/core": "^9.5.1",
    "@stryker-mutator/typescript-checker": "^9.5.1",
    "@stryker-mutator/vitest-runner": "^9.5.1",
    "@types/node": "^25.2.3",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/browser": "4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "4.0.18",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-sonarjs": "^3.0.7",
    "eslint-plugin-unicorn": "^63.0.0",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
